<?xml version="1.0"?>
<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
	<configSections>
		<section name="MerrillLynch.TeamFoundation" type="MerrillLynch.TeamFoundation.Config.SettingsSection, MerrillLynch.TeamFoundation" allowExeDefinition="MachineToLocalUser" />
	</configSections>
	<system.diagnostics>
		<sources>
			<!-- This section defines the logging configuration for My.Application.Log -->
			<source name="DefaultSource" switchName="DefaultSwitch">
				<listeners>
					<!--add name="FileLog"/-->
					<!-- Uncomment the below section to write to the Application Event Log -->
					<add name="EventLog"/>
				</listeners>
			</source>
		</sources>
		<switches>
			<add name="DefaultSwitch" value="Information" />
		</switches>
		<sharedListeners>
			<!--add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/-->
			<!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
			<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="TFSEventHandler"/>
		</sharedListeners>
	</system.diagnostics>
	<system.serviceModel>
		<bindings>
   <wsDualHttpBinding>
    <binding name="MLSecureDualHttpBinding" receiveTimeout="00:10:00"
     clientBaseAddress="http://localhost:6666/" maxReceivedMessageSize="655360">
     <readerQuotas maxStringContentLength="655360" maxArrayLength="655360" />
     <security mode="Message">
      <message clientCredentialType="Windows" negotiateServiceCredential="true"
       algorithmSuite="Default" />
     </security>
    </binding>
    <binding name="mexHttpBinding" clientBaseAddress="http://localhost:6666/">
     <security mode="Message">
      <message clientCredentialType="Windows" negotiateServiceCredential="true"
       algorithmSuite="Default" />
     </security>
    </binding>
   </wsDualHttpBinding>
   <wsHttpBinding>
    <binding name="MLSecureHttpBinding">
     <security mode="Message">
      <message clientCredentialType="Windows" negotiateServiceCredential="true"
       algorithmSuite="Default" />
     </security>
    </binding>
    <binding name="mexHttpBinding">
     <security mode="Message">
      <message clientCredentialType="Windows" negotiateServiceCredential="true"
       algorithmSuite="Default" />
     </security>
    </binding>
   </wsHttpBinding>
  </bindings>
		<services>
   <service behaviorConfiguration="MLBehavior" name="MerrillLynch.TeamFoundation.WorkItemTracking.AssignedToService">
		<endpoint address="" binding="wsHttpBinding" bindingConfiguration="MLSecureHttpBinding" contract="MerrillLynch.TeamFoundation.INotification" listenUriMode="Explicit" />
		<endpoint address="NotificationAdmin" binding="wsDualHttpBinding" bindingConfiguration="MLSecureDualHttpBinding" contract="MerrillLynch.TeamFoundation.INotificationAdmin" />
		<endpoint address="mex" binding="wsHttpBinding" bindingConfiguration="MLSecureHttpBinding" contract="IMetadataExchange" />
   </service>
   <service behaviorConfiguration="MLBehavior" name="MerrillLynch.TeamFoundation.EventAdminService">
		<endpoint address="TeamServerAdmin" binding="wsDualHttpBinding" bindingConfiguration="MLSecureDualHttpBinding" contract="MerrillLynch.TeamFoundation.ITeamServerAdmin" />
		<endpoint address="SubscriptionAdmin" binding="wsDualHttpBinding" bindingConfiguration="MLSecureDualHttpBinding" contract="MerrillLynch.TeamFoundation.ISubscriptionAdmin" />
		<endpoint address="NotificationAdmin" binding="wsDualHttpBinding"  bindingConfiguration="MLSecureDualHttpBinding" contract="MerrillLynch.TeamFoundation.INotificationAdmin" />
	    <endpoint address="EventHandlerAdmin" binding="wsDualHttpBinding"  bindingConfiguration="MLSecureDualHttpBinding" contract="MerrillLynch.TeamFoundation.IEventHandlerAdmin" />
		<endpoint address="mex" binding="wsHttpBinding" bindingConfiguration="MLSecureHttpBinding" contract="IMetadataExchange" />
   </service>
  </services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="MLBehavior">
					<serviceDebug includeExceptionDetailInFaults="true"/>
					<serviceMetadata httpGetEnabled="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
	<system.web>
		<compilation debug="true">
			<assemblies>
